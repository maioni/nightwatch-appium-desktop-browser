{
  "name": "nightwatch-desktop-browser",
  "version": "1.0.0",
  "description": "Example of a nightwatch project to run test automation for browser at desktop",
  "main": "index.js",
  "scripts": {
    "test": "npx nightwatch ./nightwatch/ --env firefox,chrome",
    "serial": "npx nightwatch ./nightwatch/ --env firefox,chrome --serial",
    "serial-headless": "npx nightwatch ./nightwatch/ --env firefox,chrome --serial --headless",
    "parallel": "npx nightwatch ./nightwatch/ --env firefox,chrome --workers auto",
    "parallel-headless": "npx nightwatch ./nightwatch/ --env firefox,chrome --workers auto --headless"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/core": "^1.3.73",
    "nightwatch": "^3.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
